<% for(var i=0; i < users.length; i++) { %>
    <tr>
        <td>
            <%= i + 1 %>
        </td>
        <td>
            <%= users[i].name %>
        </td>
        <td>
            <%= users[i].email %>
        </td>
        <td>
            <%= users[i].gender %>
        </td>
        <td>
            <%= users[i].status %>
        </td>
        <td><a class="delete" href="/update-user?id=<%= users[i]._id %>"><span><i
                        class="fa-solid fa-pen-to-square"></i></span></a> &nbsp; &nbsp; &nbsp;
            <a class="delete" data-id=<%=users[i]._id%> <span class="text-gradient"><i
                        class="fas fa-times"></i></span></a>
        </td>
    </tr>
    <% } %>